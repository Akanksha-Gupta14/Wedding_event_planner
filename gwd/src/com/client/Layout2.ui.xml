	<g:HTMLPanel>



		<m:MaterialRow addStyleNames="code">
			<!-- <m:MaterialBadge text="NEW FEATURE" textColor="WHITE" shadow="1" 
				layoutPosition="RELATIVE" float="RIGHT" backgroundColor="PINK_DARKEN_1" /> -->

			<m:MaterialNavBar width="100%"
				backgroundColor="PINK_DARKEN_3">
				<m:MaterialNavBrand marginLeft="60" href="#Test"
					textColor="WHITE" float="LEFT" text="Just For You" />
				<m:MaterialNavSection ui:field="navSection"
					float="RIGHT">
					<m:MaterialLink ui:field="home" iconType="HOME"
						paddingLeft="360" text="HOME" />
					<m:MaterialLink ui:field="r"
						iconType="ACCOUNT_CIRCLE" iconPosition="LEFT" text="REGISTRATION"
						textColor="WHITE" waves="LIGHT" activates="Akanksha" />
					<m:MaterialDropDown activator="Akanksha"
						constrainWidth="true" backgroundColor="WHITE" waves="LIGHT"
						textColor="PINK_DARKEN_3" width="40%">
						<m:MaterialLink ui:field="user" text="USER"
							backgroundColor="PINK_DARKEN_3" textColor="WHITE" waves="LIGHT"
							iconType="FACE" iconPosition="LEFT" />
						<m:MaterialLink ui:field="planner" text="PLANNER"
							backgroundColor="PINK_DARKEN_3" textColor="WHITE" waves="LIGHT"
							iconType="FACE" iconPosition="LEFT" />
					</m:MaterialDropDown>
					<m:MaterialLink ui:field="l"
						iconType="ACCOUNT_CIRCLE" iconPosition="LEFT" text="LOGIN"
						textColor="WHITE" waves="LIGHT" activates="Ak" />
					<m:MaterialDropDown activator="Ak"
						constrainWidth="true" backgroundColor="WHITE" waves="LIGHT"
						textColor="PINK_DARKEN_2" width="40%">
						<m:MaterialLink ui:field="userl" text="USER"
							backgroundColor="PINK_DARKEN_3" textColor="WHITE" waves="LIGHT"
							iconType="FACE" iconPosition="LEFT" />
						<m:MaterialLink ui:field="plannerl" text="PLANNER"
							backgroundColor="PINK_DARKEN_3" textColor="WHITE" waves="LIGHT"
							iconType="FACE" iconPosition="LEFT" />
					</m:MaterialDropDown>

					<m:MaterialDialog ui:field="dialog" type="FIXED_FOOTER"
						 inDuration="500" outDuration="500">
						<m:MaterialDialogContent>
							<m:MaterialTitle title="Just For You"
								description="Just over a year ago,we became fascinated by the idea of developing a web application where people can compare the event planner according to their location as well as budget.While their are many other way to have a dream event, we realised that there is no better way to experience the best until you have many choices."
								textColor="PINK_LIGHTEN_3" fontSize="1.25em"/>
								<m:MaterialTitle title="Contact Details"
								description="phone no:- 9876543210"
								textColor="PINK_LIGHTEN_3" fontSize="1.25em"/>
								<m:MaterialTitle title="Email address"
								description="a@gmail.com"
								textColor="PINK_LIGHTEN_3" fontSize="1.25em"/>
						</m:MaterialDialogContent>
						<m:MaterialDialogFooter>
							<m:MaterialButton ui:field="btnCloseDialog"
								text="Close Dialog" type="FLAT" />
						</m:MaterialDialogFooter>
					</m:MaterialDialog>
					<m:MaterialLink text="About us" iconType="INFO_OUTLINE"
						ui:field="btnDialog" waves="LIGHT" />
						
						
						
